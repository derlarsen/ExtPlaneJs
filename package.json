{
  "name": "extplanejs",
  "version": "1.0.0-beta",
  "description": "A JavaScript Client Library for NodeJs that uses the ExtPlane-Plugin",
  "keywords": [
    "xplane",
    "x-plane",
    "extplane",
    "extplanejs"
  ],
  "main": "ExtPlaneJs.js",
  "scripts": {
    "test": "mocha --bail --timeout 1000 --reporter spec",
    "test:coverage": "nyc --reporter=html mocha --bail --timeout 1000 --reporter spec",
    "test:watch": "mocha --bail --watch --timeout 1000 --reporter spec",
    "test:debug": "DEBUG=* npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start": "node ./example.js"
  },
  "author": "wadedos",
  "repository": {
    "type": "git",
    "url": "https://github.com/wadeos/ExtPlaneJs"
  },
  "bugs": {
    "url": "https://github.com/wadeos/ExtPlaneJs/issues"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.14",
    "base-64": "^0.1.0",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "coveralls": "^3.0.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "license": "MIT"
}
